fun fib(n: int): int =
    if n == 0 then 0
    else if n == 1 then 1
    else fib(n - 1) + fib(n - 2)

type nat = {int | _ >= 0}

method compute_fib(n: nat): (y: nat)
    ensures y == fib(n)
{
    i = 0;
    a = 1;
    b = 0;
    while i < n
    {
        c = a + b;
        a = b;
        b = c;
        i = i + 1;
    }
    return b;
}

method entry(): unit
{
    n = 0;
    while n < 10
    {
        call compute_fib(n);
        n = n + 1;
    }
    return;
}